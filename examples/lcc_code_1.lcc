def multiplicarMatrizes(){

    int linha;
    int coluna;
    int i;
    int somaprod;

    int mat1[3][3];
    mat1 = new int[3][3];
    mat1[0][0] = 1;
    mat1[0][1] = 2;
    mat1[0][2] = 3;
    mat1[1][0] = 4;
    mat1[1][1] = 5;
    mat1[1][2] = 6;
    mat1[2][0] = 7;
    mat1[2][1] = 8;
    mat1[2][2] = 9;

    int mat2[3][3];
    mat2 = new int[3][3];
    mat2[0][0] = 1;
    mat2[0][1] = 0;
    mat2[0][2] = 0;
    mat2[1][0] = 0;
    mat2[1][1] = 1;
    mat2[1][2] = 0;
    mat2[2][0] = 0;
    mat2[2][1] = 0;
    mat2[2][2] = 1;

    int mat3[3][3];

    int M1L=3, M1C=3, M2L=3, M2C=3;

    for(linha=0; linha<M1L; linha = linha + 1){

        for(coluna=0; coluna<M2C; coluna = coluna + 1){

            somaprod=0;
            for(i=0; i < M1L; i = i+1) {

                somaprod+=mat1[linha][i]*mat2[i][coluna];
            }

            mat3[linha][coluna]=somaprod;

        }
    }

    for(linha=0; linha<M1L; linha++){

        for(coluna=0; coluna<M2C; coluna++){
            print mat3[linha][coluna];
        }

    }

    return 0;
}

