
NUMEXPRESSION'''-> NUMEXPRESSION
NUMEXPRESSION -> TERM NUMEXPRESSION'
NUMEXPRESSION' -> SUM NUMEXPRESSION''
NUMEXPRESSION' -> &
NUMEXPRESSION'' -> TERM NUMEXPRESSION'

SUM -> +
SUM -> -

TERM -> UNARYEXPR TERM'
TERM' -> &
TERM' -> MULTI TERM''
TERM'' -> UNARYEXPR TERM'

MULTI -> *
MULTI -> /
MULTI -> %

UNARYEXPR -> FACTOR
UNARYEXPR -> SUM UNARYEXPR'
UNARYEXPR' -> FACTOR

FACTOR -> int_constant 
FACTOR -> float_constant 
FACTOR -> string_constant 
FACTOR -> null
FACTOR -> ( NUMEXPRESSION )

