def ordenaPorInsercao(){
    int n;
    n = 5;
    int a[5];
    a[0] = 7;
    a[1] = 9;
    a[2] = 41;
    a[3] = -12;
    a[4] = -54;

    int i;
    int j;
    int aux;

    for(i=1; i<N; i = i + 1){
        aux=a[i];
        j=i;
        for(j = j; 1; j = j){
            if(j>0){
                if (a[j-1]>aux){
                    a[j]=a[--j];
                } else{
                    break;
                }
            }else{
                break;
            }
        }
        a[j]=aux;
    }
}